  - char *{c_var} = {cast_static}char *{cast1}{c_var_cfi}->base_addr{cast2};
  - if ({cxx_var}{cxx_member}empty()) {{+
  - "ShroudStrCopy({c_var}, {c_var_cfi}->elem_len,\t {nullptr},\t 0);"
  - -}} else {{+
  - "ShroudStrCopy({c_var}, {c_var_cfi}->elem_len,\t {cxx_var}{cxx_member}data(),\t\
    \ {cxx_var}{cxx_member}size());"
  - -}}
  c_return_type: void
  c_temps:
  - cfi
  i_arg_decl:
  - 'character(len=*), intent(OUT) :: {F_string_result_as_arg}'
  i_arg_names:
  - '{F_string_result_as_arg}'
  iface_header:
  - ISO_Fortran_binding.h
  intent: function
  name: c_function_string_scalar_cfi_arg
  owner: library
