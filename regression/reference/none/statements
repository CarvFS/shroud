  - char *{c_var} = {cast_static}char *{cast1}{c_var_cfi}->base_addr{cast2};
  - size_t {c_local_trim} = ShroudLenTrim({c_var}, {c_var_cfi}->elem_len);
  - '{c_const}std::string {cxx_var}({c_var}, {c_local_trim});'
  c_temps:
  - cfi
  cxx_local_var: scalar
  i_arg_decl:
  - 'character(len=*), intent({f_intent}) :: {c_var}'
  i_arg_names:
  - '{c_var}'
  iface_header:
  - ISO_Fortran_binding.h
  intent: in
  name: c_in_string_scalar_cfi
  owner: library
