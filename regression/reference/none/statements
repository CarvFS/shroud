  - +{c_local_i} = 0,
  - '{c_local_n} = {c_var_size};'
  - -for(; {c_local_i} < {c_local_n}; {c_local_i}++) {{+
  - "{cxx_var}.push_back(\tstd::string({c_local_s},\tShroudLenTrim({c_local_s}, {c_var_len})));"
