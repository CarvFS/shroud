c_ctor_shadow_scalar_capptr:
  c_arg_decl:
  - '{c_type} * {c_var}'
  c_call:
  - "{cxx_type} *{cxx_var} =\t new {cxx_type}({C_call_list});"
  - "{c_var}->addr = static_cast<{c_const}void *>(\t{cxx_var});"
  - '{c_var}->idtor = {idtor};'
  c_return:
  - return {c_var};
  cxx_local_var: pointer
  i_arg_decl:
  - 'type({f_capsule_data_type}), intent({f_intent}) :: {c_var}'
  i_arg_names:
  - '{c_var}'
  i_module:
    iso_c_binding:
    - C_PTR
  i_module_line: '{i_module_line}'
  i_result_decl:
  - type(C_PTR) {F_result_ptr}
  i_result_var: '{F_result_ptr}'
  intent: ctor
  name: c_ctor_shadow_scalar_capptr
  owner: caller
c_ctor_shadow_scalar_capsule:
  c_arg_decl:
  - '{c_type} * {c_var}'
  c_call:
  - "{cxx_type} *{cxx_var} =\t new {cxx_type}({C_call_list});"
  - "{c_var}->addr = static_cast<{c_const}void *>(\t{cxx_var});"
  - '{c_var}->idtor = {idtor};'
  c_return_type: void
  cxx_local_var: pointer
  i_arg_decl:
  - 'type({f_capsule_data_type}), intent({f_intent}) :: {c_var}'
  i_arg_names:
  - '{c_var}'
  i_module_line: '{i_module_line}'
  intent: ctor
  name: c_ctor_shadow_scalar_capsule
  owner: caller
c_defaulttmp:
  intent: defaulttmp
  name: c_defaulttmp
  owner: library
c_defaulttmp_ctor_shadow_scalar_capptr:
  c_arg_decl:
  - '{c_type} * {c_var}'
  c_call:
  - "{cxx_type} *{cxx_var} =\t new {cxx_type}({C_call_list});"
  - "{c_var}->addr = static_cast<{c_const}void *>(\t{cxx_var});"
  - '{c_var}->idtor = {idtor};'
  c_return:
  - return {c_var};
  cxx_local_var: pointer
  i_arg_decl:
  - 'type({f_capsule_data_type}), intent({f_intent}) :: {c_var}'
  i_arg_names:
  - '{c_var}'
  i_module:
    iso_c_binding:
    - C_PTR
  i_module_line: '{i_module_line}'
  i_result_decl:
  - type(C_PTR) {F_result_ptr}
  i_result_var: '{F_result_ptr}'
  intent: defaulttmp
  name: c_defaulttmp_ctor_shadow_scalar_capptr
  owner: caller
c_dtor:
  c_call:
  - delete {CXX_this};
  - '{C_this}->addr = {nullptr};'
  c_return_type: void
  impl_header:
  - <cstddef>
  intent: dtor
  lang_c:
    impl_header:
    - <stddef.h>
  lang_cxx:
    impl_header:
    - <cstddef>
  name: c_dtor
  owner: library
c_dtor_void_scalar:
  c_call:
  - delete {CXX_this};
  - '{C_this}->addr = {nullptr};'
  c_return_type: void
  impl_header:
  - <cstddef>
  intent: dtor
  lang_c:
    impl_header:
    - <stddef.h>
  lang_cxx:
    impl_header:
    - <cstddef>
  name: c_dtor_void_scalar
  owner: library
