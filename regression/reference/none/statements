  name: py_base_ctor_array
  parse_args:
  - '{hnamefunc0}'
  - '&{value_var}'
  parse_format: O&
  post_call:
  - SH_obj->{field_name} = {cast_static}{c_type} *{cast1}{value_var}.data{cast2};
  - self->{PY_member_object} = {value_var}.obj;  // steal reference
