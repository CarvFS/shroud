  intent: function
  name: c_function_char_*_allocatable
  owner: library
c_function_char_*_arg:
  c_arg_decl:
  - char *{F_string_result_as_arg}
  - int n{F_string_result_as_arg}
  c_helper: ShroudStrCopy
  c_post_call:
  - "ShroudStrCopy({F_string_result_as_arg}, n{F_string_result_as_arg},\t {c_var},\t\
    \ -1);"
  c_return_type: void
  i_arg_decl:
  - 'character(kind=C_CHAR), intent(OUT) :: {F_string_result_as_arg}(*)'
  - 'integer(C_INT), value, intent(IN) :: n{F_string_result_as_arg}'
  i_arg_names:
  - '{F_string_result_as_arg}'
  - n{F_string_result_as_arg}
  i_module_line: iso_c_binding:C_CHAR,C_INT
  intent: function
  name: c_function_char_*_arg
  owner: library
c_function_char_*_buf_arg:
  c_arg_decl:
  - char *{F_string_result_as_arg}
  - int n{F_string_result_as_arg}
  c_helper: ShroudStrCopy
  c_post_call:
  - "ShroudStrCopy({F_string_result_as_arg}, n{F_string_result_as_arg},\t {c_var},\t\
    \ -1);"
  c_return_type: void
  i_arg_decl:
  - 'character(kind=C_CHAR), intent(OUT) :: {F_string_result_as_arg}(*)'
  - 'integer(C_INT), value, intent(IN) :: n{F_string_result_as_arg}'
  i_arg_names:
  - '{F_string_result_as_arg}'
  - n{F_string_result_as_arg}
  i_module_line: iso_c_binding:C_CHAR,C_INT
  intent: function
  name: c_function_char_*_buf_arg
  owner: library
c_function_char_*_buf_copy:
  c_arg_decl:
  - char *{c_var}
  - int {c_var_len}
  c_helper: ShroudStrCopy
  c_post_call:
  - "ShroudStrCopy({c_var}, {c_var_len},\t {cxx_var},\t -1);"
  c_return_type: void
  c_temps:
  - len
  cxx_local_var: result
  i_arg_decl:
  - 'character(kind=C_CHAR), intent({f_intent}) :: {c_var}(*)'
  - 'integer(C_INT), value, intent(IN) :: {c_var_len}'
  i_arg_names:
  - '{c_var}'
  - '{c_var_len}'
  i_module:
    iso_c_binding:
    - C_CHAR
    - C_INT
  intent: function
  name: c_function_char_*_buf_copy
  owner: library
c_function_char_*_buf_result:
  c_arg_decl:
  - char *{c_var}
  - int {c_var_len}
  c_helper: ShroudStrCopy
  c_post_call:
  - "ShroudStrCopy({c_var}, {c_var_len},\t {cxx_var},\t -1);"
  c_return_type: void
  c_temps:
  - len
  cxx_local_var: result
  i_arg_decl:
  - 'character(kind=C_CHAR), intent({f_intent}) :: {c_var}(*)'
  - 'integer(C_INT), value, intent(IN) :: {c_var_len}'
  i_arg_names:
  - '{c_var}'
  - '{c_var_len}'
  i_module:
    iso_c_binding:
    - C_CHAR
    - C_INT
  intent: function
  name: c_function_char_*_buf_result
  owner: library
