  - '! {c_var_cdesc}%elem_len = C_SIZEOF()'
  - '{c_var_cdesc}%size = {size}'
  - '{c_var_cdesc}%rank = {rank}{f_cdesc_shape}'
  f_temps:
  - cdesc
